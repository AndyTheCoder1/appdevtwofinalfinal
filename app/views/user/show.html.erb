      <option>5'11''</option>
      <option>6'</option>
      <option>6'1''</option>
      <option>6'2''</option>
      <option>6'3''</option>
      <option>6'4''</option>
      <option>6'5''</option>
      <option>6'6''</option>
      <option>6'7''</option>
    </select>
  </div>
  <div class="form-group">
    <label for="exampleFormControlSelect1">Gender</label>
    <select class="form-control" id="exampleFormControlSelect1">
      <option>Female</option>
      <option>Male</option>
      <option>Non-binary</option>
      <option>Other</option>
      <option>Prefer not to say</option>
    </select>
  </div>
  <div class="form-group">
    <label for="exampleFormControlSelect1">Drinking</label>
    <select class="form-control" id="exampleFormControlSelect1">
      <option>Yes</option>
      <option>No</option>
      <option>Sometimes</option>
      <option>Prefer not to say</option>
    </select>
  </div>
  <div class="form-group">
    <label for="exampleFormControlSelect1">Interested In:</label>
    <select class="form-control" id="exampleFormControlSelect1">
      <option>Women</option>
      <option>Men</option>
      <option>Women and Men</option>
      <option>Other</option>
      <option>Prefer not to say</option>
    </select>
