<p id="notice"><%= notice %></p>

<h1>Photos</h1>

<table>
  <thead>
    <tr>
      <th>Caption</th>
      <th>Image one</th>
      <th>Owner</th>
      <th>Location</th>
      <th>Image two</th>
      <th>Image three</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= photo.caption %></td>
        <td><%= photo.image_one %></td>
        <td><%= photo.owner_id %></td>
        <td><%= photo.location %></td>
        <td><%= photo.image_two %></td>
        <td><%= photo.image_three %></td>
        <td><%= link_to 'Show', photo %></td>
        <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
        <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_photo_path %>
